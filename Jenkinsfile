

pipeline



{



               
agent any



               
stages



               
{



                              
stage('Start the work')



                              
{



                                              
steps



                                              
{



                                                              
echo 'start example'



                                              
}



                              
}



                              
stage('Compile the work')



                              
{



                                              
steps



                                              
{



                                                              
echo 'compile'



                                              
}



                              
}



                              
stage('package the work')



                              
{



                                              
steps



                                              
{



                                                              
echo 'packages'



                                              
}



                              
}



               
}



}



